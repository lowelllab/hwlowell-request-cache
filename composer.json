{
    "name": "hwlowell/request-cache",
    "description": "A Laravel cache package for request caching with Redis and local cache support",
    "type": "library",
    "version": "1.0.1",
    "keywords": [
        "laravel",
        "cache",
        "redis",
        "request-cache"
    ],
    "homepage": "https://github.com/lowelllab/hwlowell-request-cache",
    "license": "MIT",
    "authors": [
        {
            "name": "Lowell",
            "email": "13226431320@163.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/http": "^8.50 || ^9.0 || ^10.0 || ^11.0 || ^12.0 || ^13.0",
        "illuminate/redis": "^8.50 || ^9.0 || ^10.0 || ^11.0 || ^12.0 || ^13.0",
        "illuminate/support": "^8.50 || ^9.0 || ^10.0 || ^11.0 || ^12.0 || ^13.0",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "phpunit/phpunit": "^9.0 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "HwlowellRequestCache\\": "Services/",
            "HwlowellRequestCache\\Traits\\": "Traits/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HwlowellRequestCache\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "HwlowellRequestCache\\RequestCacheServiceProvider"
            ],
            "aliases": {
                "RequestCache": "HwlowellRequestCache\\Facades\\RequestCache"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
